{
  "name": "laxmi-wallet-tracker",
  "version": "1.0.0",
  "description": "Simple Solana wallet tracker utility",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "node tests/run-tests.js",
    "test:paper": "node tests/paper-trading.test.js",
    "test:analyzer": "node tests/token-analyzer.test.js",
    "start": "node src/example.js",
    "demo": "node demo.js",
    "quick-test": "node examples/quick-test.js",
    "detailed-tx": "node examples/detailed-transactions.js",
    "monitor": "node examples/monitor-wallets.js",
    "webhooks": "node examples/webhook-demo.js",
    "telegram-bot": "node --loader ts-node/esm telegram-bot.ts",
    "telegram-bot:js": "node telegram-bot.js",
    "wallet-setup": "node examples/wallet-setup.js",
    "copy-trading": "node examples/copy-trading-bot.js",
    "bot-start": "./bot-manager.sh start",
    "bot-stop": "./bot-manager.sh stop",
    "bot-status": "./bot-manager.sh status",
    "bot-logs": "./bot-manager.sh logs"
  },
  "keywords": [
    "solana",
    "wallet",
    "tracker"
  ],
  "author": "kaupau",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@project-serum/serum": "^0.13.65",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.95.8",
    "axios": "^1.13.2",
    "bs58": "^5.0.0",
    "dotenv": "^17.2.3",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.67.0",
    "openai": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/node-telegram-bot-api": "^0.64.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
